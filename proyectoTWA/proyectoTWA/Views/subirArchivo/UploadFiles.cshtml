@{
    ViewData["Title"] = "Subir Archivo";
}

<form asp-action="UploadFiles"
      asp-controller="subirArchivo"
      method="post"
      enctype="multipart/form-data">
    <input type="file" name="files" multiple />
    <input type="submit" value="Upload Selected Files" />
</form>
